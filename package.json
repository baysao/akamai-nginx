{
  "name": "akamai-nginx",
  "version": "0.1.0",
  "description": "generate nginx config from akamai property",
  "main": "./dist/akamai-nginx.js",
  "scripts": {
    "test": "npm run build && EDGEGRID_ENV=test ./node_modules/.bin/mocha --timeout 10000 --compilers js:babel-core/register --require babel-polyfill $(find dist/test -name '*.js')",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wyvern8/akamai-nginx.git"
  },
  "author": "Andrew Vaughan <andrewv@smartcreations.com.au> (https://halt.sh)",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^4.0.0",
    "edgegrid": "^3.0.2",
    "jsonschema": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.1"
  }
}
